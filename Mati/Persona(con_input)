class Persona:
    def __init__(self, nombre, edad):
        self._nombre = nombre
        self._edad = edad


    def get_edad(self):
       return self._edad

    
    def set_edad(self, edad):
        self._edad = edad

    def cumpleaÃ±os(self):
        self._edad += 1
   

def main():
    nombre = input("Como se llama? ")
    edad = int(input("Â¿Cual es su edad? "))

    persona1 = Persona(nombre, edad)

